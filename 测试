
    
    
    
    parsers: # array
  #匹配所有订阅
  - reg: ^.*$ 
  #指定订阅 - url: 
  #- reg: - url: 订阅网站
  
  # 删除服务商提供的策略组和规则
    code: |
      module.exports.parse = (raw, { yaml }) => {
        const rawObj = yaml.parse(raw)
        const groups = []
        const rules = []
        return yaml.stringify({ ...rawObj, 'proxy-groups': groups, rules })
      } 
  # 建立自己的配置
    yaml:     
           
  #添加分组

      prepend-proxy-groups:
          - name: 🚀 节点选择
            type: select
            proxies:
                - DIRECT
                - ♻️ 自动选择
              
          - name: ♻️ 自动选择
            type: url-test
            url: http://www.gstatic.com/generate_204
            interval: 3000
            tolerance: 50

          - name: 🌍 国外
            type: select
            proxies:
                - DIRECT
                - 🚀 节点选择
                - ♻️ 自动选择  
           
          - name: 🎯 国内
            type: select
            proxies:
              - REJECT
              - DIRECT
              - 🚀 节点选择

          - name: 🛑 拦截
            type: select
            proxies:
              - REJECT
              - DIRECT
              - 🚀 节点选择


          - name: 🐟 漏网之鱼
            type: select
            proxies:
              - DIRECT
              - 🚀 节点选择

            
      commands:
            - proxy-groups.🚀 节点选择.proxies=[]proxyNames # 向指定策略组添加订阅中的节点名，可使用正则过滤
            - proxy-groups.♻️ 自动选择.proxies=[]proxyNames
            - proxy-groups.🌍 国外媒体.proxies=[]proxyNames

            
    #建立规则
